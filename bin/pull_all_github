#!/usr/bin/env bash

repos="
Chef
bronze_python
copper
updater
versions
webPOS
"

for repo in ${repos} ; do
    pushd ${repo} > /dev/null 2>&1
        echo "Updating ${repo}..."
        git pull
        git fetch --prune
        git fetch --tags
        git submodule init
        git submodule update
    popd > /dev/null 2>&1
done
