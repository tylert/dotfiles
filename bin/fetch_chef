#!/usr/bin/env bash

#wget -O 'chef-stable-ubuntu64.deb' \
curl -o 'chef-stable-ubuntu64.deb' -L \
'https://opscode.com/chef/download?v=&prerelease=false&p=ubuntu&pv=14.04&m=x86_64'

#wget -O 'chef-stable-debian64.deb' \
curl -o 'chef-stable-debian64.deb' -L \
'https://opscode.com/chef/download?v=&prerelease=false&p=debian&pv=7&m=x86_64'

#wget -O 'chef-stable-macosx64.sh' \
curl -o 'chef-stable-macosx64.dmg' -L \
'https://opscode.com/chef/download?v=&prerelease=false&p=mac_os_x&pv=10.9&m=x86_64'

