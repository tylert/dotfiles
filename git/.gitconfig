[pull]
	rebase = true
[alias]
	lol = log --oneline --decorate --graph
	lola = log --oneline --decorate --graph --all
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
